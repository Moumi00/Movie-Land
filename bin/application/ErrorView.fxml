<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<FlowPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ErrorViewController">
   <children>
      <AnchorPane prefHeight="219.0" prefWidth="421.0">
         <children>
            <VBox layoutX="78.0" layoutY="14.0" prefHeight="131.0" prefWidth="266.0">
               <children>
                  <Label fx:id="errorMessage" alignment="CENTER" contentDisplay="CENTER" prefHeight="132.0" prefWidth="274.0" text="Label" textFill="#ff1111" wrapText="true">
                     <font>
                        <Font name="Arial Bold" size="22.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <Button fx:id="closeButton" layoutX="178.0" layoutY="170.0" mnemonicParsing="false" onAction="#handleCloseButton" prefHeight="35.0" prefWidth="66.0" text="Close">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</FlowPane>
